@{
  Layout = "_Layout";
  
  foreach (var stylist in Model)
  {
    <h3><a href='/stylists/@stylist.GetId()'>@stylist.GetFirst() @stylist.GetLast()</a></h3>
  }
}

<div class='col-md-6'>
  <a href="/stylists/new">Add a new stylist</a>
</div>
<div class='col-md-6'>
  <a href="/">Return to Home</a>
</div>
