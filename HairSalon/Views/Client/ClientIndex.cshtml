@{
  Layout = "_Layout";
}

<div class='col-md-6'>
  <a href="/clients/new">Add a new client</a>
</div>
<div class='col-md-6'>
  <a href="/">Return to Home</a>
</div>

@{
  foreach (var client in Model)
  {
    <h3><a href='/stylists/@client.GetStylist()/clients/@client.GetId()'>@client.GetFirst() @client.GetLast()</a></h3>
  }
}
